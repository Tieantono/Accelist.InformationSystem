@model Accelist.InformationSystem.APILayer.Models.ViewModels.RegisterModel

<h2 class="register-sub-header"><strong>Permata Bank Data</strong></h2>

<div class="form-group">
    @Html.LabelFor(Q => Q.EmployeePermataAccountId, "Account Number", new { @class = "col-sm-2 control-label" })
    <div class="col-sm-10">
        @Html.TextBoxFor(Q => Q.EmployeePermataAccountId, new { @class = "form-control" })
        @Html.ValidationMessageFor(Q => Q.EmployeePermataAccountId, "", new { @class = "error-msg" })
    </div>
</div>

<div class="form-group">
    @Html.LabelFor(Q => Q.EmployeePermataAccountName, "Account Name", new { @class = "col-sm-2 control-label" })
    <div class="col-sm-10">
        @Html.TextBoxFor(Q => Q.EmployeePermataAccountName, new { @class = "form-control" })
        @Html.ValidationMessageFor(Q => Q.EmployeePermataAccountName, "", new { @class = "error-msg" })
    </div>
</div>

<div class="list-inline pull-right">
    <button id="workshopBtn" type="button" class="btn btn-default">
        Prev
    </button>
    <button id="otherBtn" type="button" class="btn btn-default">
        Next
    </button>
</div>