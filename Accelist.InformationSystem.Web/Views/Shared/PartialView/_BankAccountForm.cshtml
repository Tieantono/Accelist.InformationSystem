@model Accelist.InformationSystem.APILayer.Models.ViewModels.RegisterModel


<div class="panel panel-default">
    <div class="panel-heading">
        <div class="panel-title panel-title-blue">
            <h2>
                <strong>Bank Data</strong>
            </h2>
        </div>
    </div>
    <div class="panel-body">
        <div class="form-group">
            @Html.LabelFor(Q => Q.BankName, "Bank Name", new { @class = "col-xs-2 control-label" })
            <div class="col-xs-10">
                @Html.DropDownListFor(Q => Q.BankName, new SelectList(Model.BankName), new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(Q => Q.BankAccount, "Account Number", new { @class = "col-xs-2 control-label" })
            <div class="col-xs-10">
                @Html.TextBoxFor(Q => Q.BankAccount, new { @class = "form-control" })
                @Html.ValidationMessageFor(Q => Q.BankAccount, "", new { @class = "error-msg" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(Q => Q.BankAccount, "Holder Name", new { @class = "col-xs-2 control-label" })
            <div class="col-xs-10">
                @Html.TextBoxFor(Q => Q.BankAccount, new { @class = "form-control" })
                @Html.ValidationMessageFor(Q => Q.BankAccount, "", new { @class = "error-msg" })
            </div>
        </div>
    </div>
</div>